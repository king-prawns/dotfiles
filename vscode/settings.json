{
  "window.zoomLevel": 0,
  "editor.fontSize": 17,
  "files.associations": {
    "*.vue": "vue"
  },
  "breadcrumbs.enabled": true,
  "workbench.iconTheme": "eq-material-theme-icons",
  "editor.renderWhitespace": "all",
  "editor.fontFamily": "'Fira Code iScript', Consolas",
  "editor.fontLigatures": true,
  "editor.lineHeight": 24,
  "explorer.decorations.badges": false,
  "editor.tabSize": 2,
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#FF4081",
    "editorIndentGuide.activeBackground": "#FF4081",
    "list.activeSelectionForeground": "#FF4081",
    "list.inactiveSelectionForeground": "#FF4081",
    "list.inactiveSelectionBackground": "#FFFFFF10",
    "list.highlightForeground": "#FF4081",
    "scrollbarSlider.activeBackground": "#FF408150",
    "editorSuggestWidget.highlightForeground": "#FF4081",
    "textLink.foreground": "#FF4081",
    "progressBar.background": "#FF4081",
    "pickerGroup.foreground": "#FF4081",
    "tab.activeBorder": "#FF4081",
    "notificationLink.foreground": "#FF4081"
  },
  "editor.tokenColorCustomizations": {
    "[Material Theme High Contrast]": {
      "textMateRules": [
        {
          "scope": [
            "comment",
            "punctuation.definition.comment"
          ],
          "settings": {
            "foreground": "#FF8C00"
          }
        },
        {
          "scope": [
            "comment",
            "comment.block",
            "comment.line",
            "storage.type",
            "storage.modifier",
            "entity.other.attribute-name",
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "comment.block.documentation",
            "storage.type.function.arrow",
            "storage.type.generic",
            "entity.other.attribute-name.class"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    }
  },
  "javascript.validate.enable": false,
  "typescript.autoImportSuggestions.enabled": false,
  "flow.runOnEdit": false,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": false,
  "[javascript]": {},
  "workbench.colorTheme": "Material Theme High Contrast",
  "gitlens.keymap": "chorded",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": true,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "workbench.sideBar.location": "right",
  "workbench.settings.editor": "json",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true
  }
}
